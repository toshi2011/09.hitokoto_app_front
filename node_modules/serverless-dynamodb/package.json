{
    "name": "serverless-dynamodb",
    "version": "0.2.56",
    "description": "Serverless plugin to run DynamoDB local",
    "license": "MIT",
    "author": "Adam Jones (domdomegg)",
    "repository": {
        "type": "git",
        "url": "https://github.com/raisenational/serverless-dynamodb"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "vitest run",
        "test:watch": "vitest --watch",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
        "clean": "rm -rf dist",
        "build": "tsc --project tsconfig.build.json",
        "prepublishOnly": "npm run clean && npm run build"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.428.0",
        "@aws-sdk/lib-dynamodb": "^3.428.0",
        "aws-dynamodb-local": "^0.0.11"
    },
    "devDependencies": {
        "@tsconfig/node-lts": "^20.1.0",
        "@tsconfig/strictest": "^2.0.2",
        "@types/node": "^20.10.5",
        "@types/serverless": "^3.12.19",
        "eslint": "^8.56.0",
        "eslint-config-domdomegg": "^1.2.3",
        "typescript": "^5.3.3",
        "vitest": "^1.0.4"
    },
    "keywords": [
        "serverless framework plugin",
        "serverless applications",
        "serverless plugins",
        "api gateway",
        "lambda",
        "dynamodb",
        "dynamodb local",
        "aws",
        "aws lambda",
        "aws dynamodb",
        "amazon",
        "amazon web services",
        "serverless.com"
